{
	"name": "pozitronik/yii2-relations",
	"description": "Relational attributes support for YII2 ActiveRecord",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"activerecord",
		"trait",
		"relation"
	],
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Pavel Dubrovsky",
			"email": "antikillerxxl@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0",
		"pozitronik/yii2-helpers": "^1.0.0"
	},
	"require-dev": {
		"yiisoft/yii2": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"pozitronik\\relations\\": "src"
		}
	},
	"config": {
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
